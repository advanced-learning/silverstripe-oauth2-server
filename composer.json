{
    "name": "advanced-learning/silverstripe-oauth2-server",
    "description": "SilverStripe module for an oauth2 server",
    "type": "silverstripe-vendormodule",
    "license": "MIT",
    "authors": [
        {
            "name": "Conrad Dobbs",
            "email": "riddler400@gmail.com"
        }
    ],
    "require": {
        "silverstripe/framework": "^4.0",
        "league/oauth2-server": "^6.0",
        "silverstripe/vendor-plugin": "^1.0",
        "robbie/psr7-adapters": "dev-vendor",
        "PHP": ">=7.1",
        "silverstripe/admin": "^1.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/advanced-learning/psr7-adapters"
        }
    ],
    "require-dev": {
        "phpunit/PHPUnit": "^5.7@stable",
        "silverstripe/sqlite3": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "AdvancedLearning\\Oauth2Server\\": "src",
            "AdvancedLearning\\Oauth2Server\\Tests\\": "tests"
        }
    }
}
